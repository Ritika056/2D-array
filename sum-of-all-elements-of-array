#include <stdio.h>
int main()
{
    int x[3][3],sum,i,j;
    printf("enter elements of array: ");
    for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
        {
            scanf("%d", &x[i][j]);
        }
    }
    printf("elements of array: \n");
    for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
        {
            printf("%d\t", x[i][j]);
          sum=sum+x[i][j];
        }printf("\n");
    }
    printf("sum of all elements of array: %d", sum);   
    return 0;
}
