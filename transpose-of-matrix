#include <stdio.h>
int main()
{
     int a[4][4],t[4][4],i,j;
     printf("Enter Elements for Matrix of Size 3*3: ");
     for(i=0;i<=2;i++)
          for(j=0;j<=2;j++)
          {
               scanf("%d",&a[i][j]);
          }
     printf("3*3 Matrix: \n");
     for(i=0;i<=2;i++)
     {
          for(j=0;j<=2;j++)
          {
               printf("%d ",a[i][j]);
          }
          printf("\n");
     }
     for(i=0;i<=2;i++)
          for(j=0;j<=2;j++)
          {
               t[j][i]=a[i][j]; // Transpose the matrix
          }
     /* Print the transpose matrix */
     printf("Transpose of Matrix: \n");
     for(i=0;i<=2;i++)
     {
          for(j=0;j<=2;j++)
          {
               printf("%d ",t[i][j]);
          }
     printf("\n");
     }
     return 0;
}
